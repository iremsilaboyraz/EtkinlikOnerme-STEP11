import java.util.Scanner;

public class Etkinlik {
    public static void main(String[] args){
        int heat;

        Scanner input=new Scanner(System.in);

        System.out.printf("Sıcaklık değerini giriniz:");
        heat= input.nextInt();
        
        if (heat<5){
            System.out.printf("Kayak yapabilirsiniz");
            
        }else if (heat>=5 && heat<=10) {
            System.out.printf("Sinemaya gidebilirsiniz");

        }else if (heat>=10 && heat<=15) {
            System.out.printf("Sinemaya veya pikniğe gidebilirsiniz");

        }else if (heat>=15 && heat<=25){
            System.out.printf("Pikniğe gidebilirsiniz");
        }else
            System.out.printf("Yüzmeye gidebilirsiniz");
    }
}
